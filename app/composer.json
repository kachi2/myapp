{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "blockchain/api-v1-client-php",
                "version": "dev-master",
                "source": {
                    "url": "https://github.com/blockchain/api-v1-client-php",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-bcmath": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "blockchain/api-v1-client-php": "dev-master",
        "fideloper/proxy": "^4.0",
        "gerardojbaez/money": "^0.3.1",
        "guzzlehttp/guzzle": "^6.5",
        "intervention/image": "^2.5",
        "kevupton/laravel-coinpayments": "^1.1",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0",
        "rukhsar/laravel-active-route": "^1.4",
        "uxweb/sweet-alert": "^2.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Blockchain\\": "vendor/blockchain/api-v1-client-php/src/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
